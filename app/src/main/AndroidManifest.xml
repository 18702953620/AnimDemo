        </activity>
        <activity android:name=".base.ScaleActivity" />
        <activity android:name=".base.BaseActivity" />
        <activity android:name=".base.AlphaActivity" />
        <activity android:name=".base.TranslateActivity" />
        <activity android:name=".base.RotateActivity" />
        <activity android:name=".base.SetActivity" />
        <activity android:name=".base.InterpolatorActivity" />
        <activity android:name=".value.ValueActivity" />
        <activity android:name=".object.ObjectActivity" />
        <activity android:name=".demo.BallActivity" />
        <activity android:name=".demo.DemoActivity" />
        <activity android:name=".demo.WXPicActivity" />
